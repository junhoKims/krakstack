{
  "name": "krakstack",
  "version": "1.0.0",
  "description": "각종 패키지 및 코드베이스 모노레포",
  "author": "junhoKims",
  "scripts": {
    "build": "turbo build",
    "build:storybook": "turbo build:storybook",
    "build:docs": "turbo build:docs",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check:lint": "turbo check:lint",
    "check:type": "turbo check:type",
    "test": "turbo test",
    "clean": "rm -rf node_modules && pnpm -r exec rm -rf node_modules && pnpm i --fix-lockfile",
    "clean:cache": "rm -rf .turbo .eslintcache && pnpm -r exec rm -rf .turbo .eslintcache"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.3",
    "@krakstack/eslint-config": "workspace:^",
    "@krakstack/typescript-config": "workspace:^",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/addon-themes": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/builder-vite": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/test": "8.6.12",
    "@storybook/test-runner": "0.22.0",
    "@storybook/testing-library": "0.2.2",
    "@svgr/webpack": "8.1.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "2.1.8",
    "autoprefixer": "10.4.20",
    "eslint": "9.39.2",
    "jsdom": "25.0.1",
    "postcss": "8.4.49",
    "postcss-import": "16.1.0",
    "postcss-nesting": "13.0.1",
    "prettier": "3.8.1",
    "rollup": "4.28.1",
    "storybook": "8.6.12",
    "tailwind-merge": "2.6.0",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "tsc-alias": "1.8.10",
    "turbo": "2.8.8",
    "typescript": "5.7.2",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "2.1.8"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=25.6.0"
  }
}

{
  "name": "krak",
  "version": "1.0.0",
  "description": "krak workspace",
  "author": "junhoKims",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check:lint": "turbo check:lint",
    "clean": "rm -rf node_modules && pnpm -r exec rm -rf node_modules && pnpm -r exec rm -rf .next && pnpm store prune && pnpm install",
    "clean:turbo": "rm -rf .turbo && pnpm -r exec rm -rf .turbo"
  },
  "devDependencies": {
    "@krak/eslint-config": "workspace:^",
    "@krak/typescript-config": "workspace:^",
    "autoprefixer": "10.4.20",
    "eslint": "9.16.0",
    "postcss": "8.4.49",
    "postcss-import": "16.1.0",
    "prettier": "3.4.1",
    "rollup": "4.28.1",
    "tailwindcss": "3.4.1",
    "tsc-alias": "1.8.10",
    "turbo": "2.3.3",
    "typescript": "5.7.2"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "node": "20.14.0",
    "pnpm": "9.14.4"
  }
}
